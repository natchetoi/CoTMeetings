<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="fusion.controller.LoginView">
	<App>
		<pages>
			<Page class="coTLoginView" showSubHeader="false" showHeader="false" enableScrolling="false">
				<content>
				    <Link id="__link0"/>
				    <Label text="User Name" width="100%" id="__label0" class="coTUserNameLabel"/>
				    <Input width="50%" id="__input0" value="ynatche" class="coTUserName"/>
				    <Label text="Password" width="100%" id="__label1" class="coTPasswordLabel"/>
				    <Input width="50%" id="__input1" value="******" type="Password" class="coTPassword"/>
				    <HBox width="100%">
				    	<Label text="Remember Me" width="50%" id="__label2" class="coTRememberMeLabel"/>
				    	<CheckBox id="__box0" text="" class="coTRememberMe" width="90%"/>
				    	
				    </HBox>
				    
				    <HBox width="100%" id="__hbox0" alignContent="End" alignItems="Start">
				        <items>
				            <core:InvisibleText text="InvisibleText" id="__text0" width="100px" />
				            <Button text="Login" width="160px" id="loginButtonId" activeIcon="sap-icon://log" icon="sap-icon://log" press="onLogin" class="coTLoginButton"  type="Emphasized" />
				        </items>
				    </HBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>