<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="fusion.controller.EmployeeDetails">
	<App>
		<pages>
			<Page title="Title">
				<content>
				    <ObjectHeader xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" title="{MeetingSubject}" number="{Start}" numberUnit="{RoomID}" id="__header0">
				        <ObjectAttribute text="{MeetingSubject}"/>
				        <ObjectAttribute text="{MeetingComment}"/>
				        </ObjectHeader>
			<IconTabBar select="onDetailSelect" id="idIconTabBar">
				<items>
					<IconTabFilter key="API_Room" text="Attendee" icon="sap-icon://hint">
						<!--key="Detail" -->
						<content>
							<core:Fragment fragmentName="fusion.view.DetailInfoForm" type="XML"/>
						</content>
					</IconTabFilter>
				</items>
			</IconTabBar>
				        
				</content>
		<footer>
			<Toolbar>
				
			</Toolbar>
		</footer>				
				
			</Page>
		</pages>
	</App>
</mvc:View>